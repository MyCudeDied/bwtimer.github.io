{
  "manifest_version": 2,
  "name": "CSTimer Makeover",
  "version": "0.1",
  "content_scripts": [{
    "matches": ["https://cstimer.net/*", "http://cstimer.net/*"],
    "js": ["script.js"],
    "run_at": "document_end"
  }],
  "web_accessible_resources": ["styles.css", "img/favicon.png"],
  "icons": {
    "128": "img/cube.png"
  }
}
